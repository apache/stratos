            "type":"string",
            "id": "root/active",
            "default": "true",
            "required":false
        },
        "adminPassword": {
            "type":"string",
            "id": "root/adminPassword",
            "default": "admin123",
            "required":false
        },
        "admin": {
            "type":"string",
            "id": "root/admin",
            "default": "admin",
            "required":false

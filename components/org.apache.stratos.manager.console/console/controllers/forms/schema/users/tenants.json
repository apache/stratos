            "required":false
        },
        "firstname": {
            "type":"string",
            "id": "root/firstname",
            "default": "Frank",
            "required":false
        },
        "lastname": {
            "type":"string",
            "id": "root/lastname",
            "default": "Myers",
            "required":false
        },
        "tenantDomain": {
            "type":"string",
            "id": "root/tenantDomain",
            "default": "frank.com",
            "required":false
